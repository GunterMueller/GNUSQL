LN_S="ln -s"                             
topdir=`pwd`                                 
                                             
#-------------------------------------------#
fix_dir () {                                 
  sfn=$1     ## configurable file            
  dir=$2     ## target subdirectory          
  tfn=$3     ## original template destination
                                             
 ( # go to sub shell                         
   ## cd to target directory                 
   cd $dir ## cd to target directory         
   if [ ! -r ${sfn}.in ]; then               
     ${LN_S} $tfn ${sfn}.in                  
   fi                                        
   if [ -x ${tfn} ]; then                    
     chmod a+x  ${sfn}                       
   fi                                        
 )                                           
                                             
}                                            
